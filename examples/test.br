struct Combinable<T> {
  data: T;

  fn setData(data: T) -> Self {
    self.data = data;
    return self;
  }

  fn combine(other: T) -> T {
    self.data = self.data + other;
    return self.data;
  }

  static fn new(init: T) -> Self {
    return Self {
      data: init,
    };    
  }
}

fn main () {
  const c1 = Combinable<number>::new(10.5).setData(10);
  const numbers = [1, 2, 3, 4];
}