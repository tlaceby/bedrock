// main.br

struct Circle {
    radius: number;

    fn radiusPlusTen () -> number {
        return self.radius + 10;
    }

    static fn new (r: number) -> Circle {
        return Circle {
            radius: r,
        };
    }
}

const c1: Circle = Circle{
    radius: 10.2,
};

const c2 = Circle::new;