fn add <T> (a: T, b: T) -> T {
  return a + b;
}

struct Combinable<T> {
  data: T;

  fn setData(data: T) -> Self {
    self.data = data;
    return self;
  }

  fn combine(other: T) -> T {
    self.data = self.data + other;
    return self.data;
  }

  static fn new(init: T) -> Self {
    return Self {
      data: init,
    };    
  }
}

