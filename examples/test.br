// main.br

struct Circle {
    radius: number;

    fn radiusPlusN (n: number) -> number {
        return self.radius + n;
    }

    static fn new (r: number) -> Circle {
        return Circle {
            radius: r,
        };
    }
}

const c1: Circle = Circle{
    radius: 10.2,
};

const r = Circle::new(10).radiusPlusN(10);